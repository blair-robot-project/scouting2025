          "required": true,
          "code": "ded",
          "formResetBehavior": "reset",
          "defaultValue": "NA",
          "choices": {
            "NA": "None",
            "cs": "Got a coral stuck",
            "ba": "Beached on an algae",
            "di": "Disabled(Orange light fully on)",
            "de": "Dead(Orange light fully off)",
            "t": "Tipped over"
